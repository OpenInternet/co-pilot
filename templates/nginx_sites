server {
       listen 8080;
       location / {
                proxy_pass http://127.0.0.1:8000;
       }
       location /static {
        alias  /home/www/copilot/copilot/static/;
      }
}

server {
    listen 80;
    location / {
        proxy_pass http://127.0.0.1:8090;
    }
    location /config {
        proxy_pass http://127.0.0.1:8000;
    }
}
